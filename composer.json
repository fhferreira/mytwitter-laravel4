{
	"require": {
		"laravel/framework": "4.0.*"
	},
	"require-dev": {
		"way/generators" : "dev-master",
		"mockery/mockery" : "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/repositories"
		]
	},
	"scripts": {
		"post-update-cmd": "php artisan optimize"
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
